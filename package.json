{
  "name": "unrefresh",
  "type": "module",
  "version": "0.0.0-alpha.1",
  "packageManager": "pnpm@7.12.0",
  "description": "",
  "author": "Song wuk <1017301325@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/songwuk/unrefresh#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/songwuk/unrefresh.git"
  },
  "bugs": "https://github.com/songwuk/unrefresh/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./css": {
      "default": "./dist/css/container.css"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "play": "vite playground",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.0",
    "@antfu/utils": "^0.5.2",
    "@rollup/plugin-image": "^2.1.1",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "pnpm": "^7.12.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-css-porter": "^1.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.2",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "postcss": "^8.4.17",
    "rollup-plugin-postcss": "^4.0.2"
  }
}
